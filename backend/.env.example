NODE_ENV=development
PORT=5001

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=tradervue_db
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# JWT
JWT_SECRET=your_super_secret_jwt_key_here
JWT_EXPIRE=7d

# Email
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASS=your_app_password

# Frontend URL
FRONTEND_URL=http://localhost:5173

# File Upload (50MB limit for large CSV files)
MAX_FILE_SIZE=52428800

# External API Keys (Optional)

# Google Gemini API Key - For AI-powered CUSIP to ticker symbol lookups
# Get your free API key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

# OpenFIGI API Key - Get one free at https://www.openfigi.com/api
# Provides better rate limits for CUSIP to ticker symbol lookups
OPENFIGI_API_KEY=

# Alpha Vantage API Key (Optional) - For backup symbol lookups
ALPHA_VANTAGE_API_KEY=